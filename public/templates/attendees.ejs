<ul>
	<% for(var i=0; i<attendees.length; i++){ 
		attendee = attendees[i];
		%>

		<li>
			<img src="<%= attendee.avatar %>"/>
			<h2><%= attendee.firstName %> <%= attendee.lastName %></h2>
				<% if(attendee.twitter){ %><p><a href="http://twitter.com/<%= attendee.twitter %>">@<%= attendee.twitter %></a></p>
				<% } %>
				<% if(attendee.github){ %><p><a href="http://github.com/<%= attendee.github %>">github.com/<%= attendee.github %></a></p> 
				<% } %>
		</li>

	<% } %>
</ul>
			
